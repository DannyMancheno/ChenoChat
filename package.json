{
  "name": "chenochat",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --quiet --exec babel-node server.js --ignore client",
    "startClient": "npm start --prefix client",
    "concStart": "concurrently \"npm run start\" \"npm run startClient\"",
    "build": "babel server.js --out-file server.compiled.js",
    "buildClient": "npm run build --prefix client",
    "concBuild": "concurrently \"npm run build\" \"npm run buildClient\"",
    "EBDeployStage": "npm run concBuild && git add . && eb deploy --staged && eb open && echo 'Built, Staged and Deployed to EB' "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.6",
    "@types/jquery": "^3.5.29",
    "nodemon": "^3.0.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fetch": "^1.1.0",
    "isomorphic-fetch": "^3.0.0",
    "jwt-simple": "^0.5.6",
    "mysql": "^2.18.1"
  }
}
